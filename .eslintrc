{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "indent": [2, 4, { "SwitchCase": 1, "VariableDeclarator": {"var": 2, "let": 2, "const": 3} }],
        "no-restricted-syntax": 0,
        "react/jsx-indent-props": [2, 4],
        "react/jsx-closing-bracket-location": [2, {"selfClosing": false}]
    }
}